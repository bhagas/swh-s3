<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="shortcut icon" href="/images/template/shortcut_icon.ico">
    <title></title>

    <!-- <link href="/stylesheets/template/css/theme.css" rel="stylesheet"> -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css"
        integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">
    <!-- <link href="/stylesheets/template/css/style.css" rel="stylesheet"> -->
    <link href="/stylesheets/template/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.2/css/bootstrap.css" rel="stylesheet" type="text/css">
    
    
    <!-- Plugin This Page -->
    

    
       <script src="/plugins/jquery/jquery-1.11.1.min.js"></script>
       
    <!-- End Plugins This Page -->

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    <style type="text/css">
        html {
            font-family: "avenir";

        }

        @font-face {
            font-family: "avenir";
            src: url('/stylesheets/template/fonts/Avenir.ttf');
        }

        .table-responsive{
          width: 96% !important;
        }
    </style>
</head>

<body>
  <header style="background-color: seagreen;padding-top: 10px;padding-bottom: 10px;">
    <div class="container">
      <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
          <h5 style="color: #fff;text-align: center;"><strong>DINAS PERTANIAN KOTA SALATIGA</strong></h5>
        </div>
      </div>
    </div>
  </header>
    <div class="container">
        <div class="row" style="margin-top: 30px;">
          <div class="col-xs-12 col-sm-12">
            <h4 class="text-center"><strong>KALENDER TANAM</strong></h4>
            <ul class="list-inline text-center" style="margin-bottom: 10px;">
              <li class="list-inline-item"><a href="/surveyor/dashboard">DASHBOARD</a></li>
              <li class="list-inline-item">|</li>
              <li class="list-inline-item"><a href="/surveyor/list">DATA LPPB</a></li>
              <li class="list-inline-item">|</li>
              <li class="list-inline-item"><strong>KALENDER TANAM</strong></li>
            </ul>
            <hr/>
          </div>
        </div>
        <div class="row">
          <form method="POST" action="/surveyor/submit_katam">
            <input type="hidden" class="form-control" name="detailLahanId" id="" value="<%= data[0].id%>">
              <div class="col-xs-12 col-sm-12">
                <div class="row">
                  <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 table-no-border">
                    <table class="table table-borderless m-b-0">
                      <tbody>
                        <tr>
                          <td style="width: 160px;">No. Bidang</td>
                          <td style="width: 1px;">:</td>
                          <td><%= data[0].no_bidang%></td>
                        </tr>
      
                        <tr>
                          <td>Alamat</td>
                          <td>:</td>
                          <td><%= data[0].alamat%></td>
                        </tr>
      
                        <tr>
                          <td>Kecamatan</td>
                          <td>:</td>
                          <td><%= data[0].kec%></td>
                        </tr>
      
                        <tr>
                          <td>Kelurahan</td>
                          <td>:</td>
                          <td><%= data[0].kel%></td>
                        </tr>

                        <tr>
                          <td style="width: 160px;">Pemilik</td>
                          <td  style="width: 1px;">:</td>
                          <td><%= data[0].pemilik%></td>
                        </tr>
      
                        <tr>
                          <td>Jenis Lahan</td>
                          <td>:</td>
                          <td><%= data[0].jenis_lahan%></td>
                        </tr>
      
                        <tr>
                          <td>Status Lahan</td>
                          <td>:</td>
                          <td><%= data[0].status_lahan%></td>
                        </tr>
      
                        <tr>
                          <td>Luas</td>
                          <td>:</td>
                          <td><%= data[0].luas%></td>
                        </tr>
                      </tbody>
                    </table>
                  </div>
                </div>

                <div class="row">
                  <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <hr>
                  </div>
                </div>

                <div class="row">
                  <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <div class="form-group">
                      <label>Komoditas</label>
                      <div class="row">
                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                          <select class="form-control" name="komoditaId" id="" data-search="true">
                          <% for(var i=0;i<komoditas.length; i++){%>
                            <option value="<%= komoditas[i].id%>"><%= komoditas[i].nama%></option>
                            <%}%>
                          </select>
                        </div>
                      </div>
                    </div>
      
                    <div class="form-group">
                      <label>Mulai Tanam</label>
                      <div class="row">
                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                          <select class="form-control" name="bulan_awal" id="">
                          <option value="">-- Pilih --</option>  
                          <option value="1">Januari</option>
                          <option value="2">Februari</option>
                          <option value="3">Maret</option>
                          <option value="4">April</option>
                          <option value="5">Mei</option>
                          <option value="6">Juni</option>
                          <option value="7">Juli</option>
                          <option value="8">Agustus</option>
                          <option value="9">September</option>
                          <option value="10">Oktober</option>
                          <option value="11">November</option>
                          <option value="12">Desember</option>
                          </select>
                        </div>
                      </div>
                    </div>
      
                    <div class="form-group">
                      <label>Akhir Tanam</label>
                      <div class="row">
                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                          <select class="form-control" name="bulan_akhir" id="">
                          <option value="">-- Pilih --</option>  
                          <option value="1">Januari</option>
                          <option value="2">Februari</option>
                          <option value="3">Maret</option>
                          <option value="4">April</option>
                          <option value="5">Mei</option>
                          <option value="6">Juni</option>
                          <option value="7">Juli</option>
                          <option value="8">Agustus</option>
                          <option value="9">September</option>
                          <option value="10">Oktober</option>
                          <option value="11">November</option>
                          <option value="12">Desember</option>
                          </select>
                        </div>
                      </div>
                    </div>

                    <div class="form-group">
                      <label>Produktifitas (ton/hektar)</label>
                      <div class="row">
                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                          <input type="text" class="form-control" name="ton" id="">
                        </div>
                      </div>
                    </div>
      
                    <div class="form-group">
                      <label>Tahun</label>
                      <div class="row">
                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                          <select class="form-control" name="tahun" data-search="true">
                            <%for(var c = 2018; c <=2024; c++){%>
                            <option><%=c%></option>
                            <%}%>
                          </select>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <button class="btn btn-warning btn-embossed" style="margin-bottom: 0px;" type="submit">Tambah</button>
                  </div>
                </div>
              </div>
          </form>
        </div>

        <div class="row" style="margin-top: 30px;">
          <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 table-responsive">
            <table class="table table-bordered">
              <thead>
                <tr>
                  <th>Komoditas</th>
                  <th>Mulai Tanam</th>
                  <th>Akhir Tanam</th>
                  <th>Produktifitas (ton/hektar)</th>
                  <th>Tahun</th>
                  <th>Option</th>
                </tr>
              </thead>
              <tbody>
                <%for(var i=0;i<katam.length; i++){%>
                <tr>
                  <td><%= katam[i].komodita.nama%></td>
                  <td><%= katam[i].bulan_awal%></td>
                  <td><%= katam[i].bulan_akhir%></td>
                  <td><%= katam[i].ton%></td>
                  <td><%= katam[i].tahun%></td>
                  <td><a class="btn btn-danger btn-embossed btn-sm"
                    href="/surveyor/delete_katam/<%= katam[i].id%>/<%= data[0].id%>" role="button"
                    onclick="return confirm('Apakah anda yakin untuk menghapus data ini ?');"><i
                      class="fa fa-trash-o" aria-hidden="true"></i></a></td>
                </tr>
                <%}%>
              </tbody>
            </table>
          </div>
        </div>
    </div>

    
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"
        integrity="sha384-B4gt1jrGC7Jh4AgTPSdUtOBvfO8shuf57BaghqFfPlYxofvL8/KUEfYiJOMMV+rV"
        crossorigin="anonymous"></script>
    <!-- Plugins This Page -->

    <!-- End Plugins This Page -->
    <script type="text/javascript">
      
    </script>
</body>

</html>