        html {
            font-family: "avenir";

        }

        @font-face {
            font-family: "avenir";
            src: url('/stylesheets/template/fonts/Avenir.ttf');
        }

        .table-responsive{
          width: 96% !important;
        }
    </style>
</head>

<body>
  <header style="background-color: seagreen;padding-top: 10px;padding-bottom: 10px;">
    <div class="container">
      <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
          <h5 style="color: #fff;text-align: center;"><strong>DINAS PERTANIAN KOTA SALATIGA</strong></h5>
        </div>
      </div>
    </div>
  </header>
    <div class="container">
        <div class="row" style="margin-top: 30px;">
          <div class="col-xs-12 col-sm-12">
            <h4 class="text-center"><strong>PENGURANGAN LAHAN</strong></h4>
            <ul class="list-inline text-center" style="margin-bottom: 10px;">
              <li class="list-inline-item"><a href="/surveyor/dashboard">DASHBOARD</a></li>
              <li class="list-inline-item">|</li>
              <li class="list-inline-item"><a href="/surveyor/list">DATA LPPB</a></li>
              <li class="list-inline-item">|</li>
              <li class="list-inline-item"><strong>PENGURANGAN LAHAN</strong></li>
            </ul>
            <hr/>
          </div>
        </div>
        <div class="row">
          <div class="col-xs-12 col-sm-12">
            <div class="row">
              <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 table-no-border">
                <table class="table table-bordereless m-b-0">
                  <tbody>
                    <tr>
                      <td style="width: 160px;">No. Bidang</td>
                      <td style="width: 1px;">:</td>
                      <td><%= data[0].no_bidang%></td>
                    </tr>
  
                    <tr>
                      <td>Alamat</td>
                      <td>:</td>
                      <td><%= data[0].alamat%></td>
                    </tr>
  
                    <tr>
                      <td>Kecamatan</td>
                      <td>:</td>
                      <td><%= data[0].kec%></td>
                    </tr>
  
                    <tr>
                      <td>Kelurahan</td>
                      <td>:</td>
                      <td><%= data[0].kel%></td>
                    </tr>
                    <tr>
                      <td style="width: 160px;">Pemilik</td>
                      <td  style="width: 1px;">:</td>
                      <td><%= data[0].pemilik%></td>
                    </tr>
  
                    <tr>
                      <td>Jenis Lahan</td>
                      <td>:</td>
                      <td><%= data[0].jenis_lahan%></td>
                    </tr>
  
                    <tr>
                      <td>Status Lahan</td>
                      <td>:</td>
                      <td><%= data[0].status_lahan%></td>
                    </tr>
  
                    <tr>
                      <td>Luas</td>
                      <td>:</td>
                      <td><%= data[0].luas%></td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>

            <div class="row">
              <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <hr>
              </div>
            </div>

            <div class="row">
              <form method="POST" action="/surveyor/submit_perubahan_lahan">
                <input type="hidden" class="form-control" name="detailLahanId" id="" value="<%= data[0].id%>">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                  <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                      <div class="form-group">
                        <label>Luas</label>
                        <div class="row">
                          <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                            <input type="text" class="form-control" name="luas" id="">
                          </div>
                        </div>
                      </div>
                    </div>
    
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                      <div class="form-group">
                        <label>Tahun</label>
                        <div class="row">
                          <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                            <select class="form-control" name="tahun" data-search="true">
                              <%for(var c = 2018; c <=2024; c++){%>
                              <option><%=c%></option>
                              <%}%>
                            </select>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                  <button class="btn btn-warning btn-embossed" style="margin-bottom: 0px;" type="submit">Tambah</button>
                </div>
              </form>
            </div>
          </div>
        </div>

        <div class="row" style="margin-top: 30px;">
          <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 table-responsive">
            <table class="table table-bordered">
              <thead>
                <tr>
                  <th>Luas</th>
                  <th>Tahun</th>
                  
                  <th>Option</th>
                </tr>
              </thead>
              <tbody>
                <%for(var i=0;i<perubahan_lahan.length; i++){%>
                <tr>
                  <td><%= perubahan_lahan[i].luas%></td>
                  <td><%= perubahan_lahan[i].tahun%></td>
                  <td>
                    <a class="btn btn-danger btn-embossed btn-sm"
                    href="/surveyor/delete_perubahan_lahan/<%= perubahan_lahan[i].id%>/<%= data[0].id%>" role="button"
                    onclick="return confirm('Apakah anda yakin untuk menghapus data ini ?');"><i
                      class="fa fa-trash-o" aria-hidden="true"></i></a>
                  </td>
                </tr>
                <%}%>
              </tbody>
            </table>
          </div>
        </div>
    </div>

    
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"
        integrity="sha384-B4gt1jrGC7Jh4AgTPSdUtOBvfO8shuf57BaghqFfPlYxofvL8/KUEfYiJOMMV+rV"
        crossorigin="anonymous"></script>
    <!-- Plugins This Page -->

    <!-- End Plugins This Page -->
    <script type="text/javascript">
      
    </script>
</body>

</html>